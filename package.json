{
  "name": "thefrontendcoach.com",
  "version": "1.0.0",
  "description": "A dedicated support and learning system for those wishing to start a career as a front-end developer",
  "main": "index.js",
  "scripts": {
    "compress": "gulp",
    "dev": "cd ./src && next dev",
    "build": "cd ./src && next build && next export && npm run postbuild",
    "start": "cd ./src && next start",
    "postbuild": "cp -r _redirects ./src/out/_redirects",
    "analyse": "ANALYZE=true && cd ./src && next build",
    "analyse:browser": "BUNDLE_ANALYZE=browser && cd ./src && next build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrobind/thefrontendcoach.com.git"
  },
  "keywords": [
    "front end",
    "dev",
    "web development",
    "self-taught",
    "learn to code",
    "mentor",
    "coach"
  ],
  "author": "James Robinson <jrobind@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrobind/thefrontendcoach.com/issues"
  },
  "homepage": "https://github.com/jrobind/thefrontendcoach.com#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.0.0",
    "globby": "11.0.1",
    "gulp": "4.0.2",
    "gulp-imagemin": "7.1.0",
    "node-sass": "4.14.1",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "tailwindcss": "1.9.6"
  },
  "dependencies": {
    "@next/bundle-analyzer": "10.0.1",
    "classnames": "2.2.6",
    "gray-matter": "^4.0.2",
    "next": "9.5.5",
    "next-seo": "4.14.1",
    "raw-loader": "4.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-loader-spinner": "3.1.14",
    "react-markdown": "5.0.2",
    "react-social-sharing": "3.3.0",
    "react-syntax-highlighter": "15.2.1"
  }
}
